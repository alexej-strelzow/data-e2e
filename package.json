{
  "name": "data-e2e",
  "version": "0.1.0",
  "scripts": {
    "start": "npx ts-node src/index.ts",
    "test": "jest --watch",
    "######## LINTING & FORMATING ########": "",
    "lint": "eslint src",
    "format": "npx prettier 'src/**/*.{ts,json,scss,html}' --write",
    "style": "npm run format && npm run lint"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "npx prettier --write"
    ]
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "minimist": "^1.2.5",
    "node-html-parser": "~1.2.20",
    "prompt": "~1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "~12.12.54",
    "@typescript-eslint/eslint-plugin": "~4.5.0",
    "@typescript-eslint/parser": "~4.5.0",
    "eslint": "~7.11.0",
    "eslint-import-resolver-typescript": "~2.3.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-jsdoc": "~30.7.3",
    "husky": "~4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "~10.4.2",
    "prettier": "~2.1.2",
    "ts-jest": "^26.4.4",
    "typescript": "~4.0.0"
  }
}
